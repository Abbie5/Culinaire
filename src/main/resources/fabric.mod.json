{
	"schemaVersion": 1,
	"name": "${mod_name}",
	"id": "${mod_id}",
	"version": "${version}",
	"icon": "assets/${mod_id}/textures/logo.png",
	"description": "An expansion onto the existing foodstuffs in Minecraft with intricate, well-made additions. From the innovative sandwich system to the creation of cheese in cauldrons and the brewing of tea in kettles, you won't find another food mod quite like this one.",
	"authors": [
		"Hugman",
		"Plantkillable",
		"Cydian",
		"Wintrius",
		"Dawn Team"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/${curseforge_slug}",
		"sources": "https://github.com/DawnTeamMC/${github_name}",
		"issues": "https://github.com/DawnTeamMC/${github_name}/issues"
	},
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.hugman.ce_foodstuffs.CEFoodstuffs"
		],
		"client": [
			"com.hugman.ce_foodstuffs.CEFoodstuffsClient"
		],
		"modmenu": [
			"com.hugman.ce_foodstuffs.config.CEFModMenuIntegration"
		]
	},
	"mixins": [
		"ce_foodstuffs.mixins.json"
	],
	"depends": {
		"minecraft": "*",
		"fabric": "*",
		"dawn": "*"
	},
	"custom": {
		"modupdater": {
			"strategy": "curseforge",
			"projectID": 390675
		}
	},
	"license": "MIT"
}
